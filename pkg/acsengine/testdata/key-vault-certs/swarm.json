{
  "apiVersion": "vlabs",
  "properties": {
    "orchestratorProfile": {
      "orchestratorType": "Swarm"
    },
    "masterProfile": {
      "count": 1,
      "dnsPrefix": "masterdns1",
      "vmSize": "Standard_D2_v2",
      "secrets":[
        {
          "sourceVault":{
            "id":"vaultId"
          },
          "vaultCertificates":[
            {
              "certificateUrl" :"http://url1"
            }
          ]
        }
      ]
    },
    "agentPoolProfiles": [
      {
        "name": "agentpool1",
        "count": 3,
        "vmSize": "Standard_D2_v2",
        "dnsPrefix": "agentdns1",
        "ports": [
          80,
          443,
          8080
        ]
      },
      {
        "name": "agentpool2",
        "count": 3,
        "vmSize": "Standard_D2_v2",
        "secrets":[
          {
            "sourceVault":{
              "id":"vaultId"
            },
            "vaultCertificates":[
              {
                "certificateUrl" :"http://url2"
              }
            ]
          }
        ]
      },
      {
        "name": "agentpoolw",
        "count": 3,
        "vmSize": "Standard_D2_v2",
        "osType": "Windows",
        "secrets":[
          {
            "sourceVault":{
              "id":"vaultId"
            },
            "vaultCertificates":[
              {
                "certificateUrl" :"http://url3",
                "certificateStore" : "store"
              },
              {
                "certificateUrl" :"http://url4",
                "certificateStore" : "store"
              }
            ]
          }
        ]
      }
    ],
    "windowsProfile": {
      "adminUsername": "azureuser",
      "adminPassword": "replacepassword1234$"
    },
    "linuxProfile": {
      "adminUsername": "azureuser",
      "ssh": {
        "publicKeys": [
          {
            "keyData": "ssh-rsa PUBLICKEY azureuser@linuxvm"
          }
        ]
      }
    }
  }
}
