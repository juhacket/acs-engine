{
  "apiVersion": "vlabs",
  "properties": {
    "orchestratorProfile": {
      "orchestratorType": "DCOS"
    },
    "masterProfile": {
      "count": 3,
      "dnsPrefix": "masterdns1",
      "vmSize": "Standard_D2_v2",
      "secrets":[
        {
          "sourceVault":{
            "id":"vaultId"
          },
          "vaultCertificates":[
            {
              "certificateUrl" :"http://url1"
            }
          ]
        }
      ]
    },
    "agentPoolProfiles": [
      {
        "name": "agentprivate",
        "count": 3,
        "vmSize": "Standard_D2_v2"
      },
      {
        "name": "agentpublic",
        "count": 3,
        "vmSize": "Standard_D2_v2",
        "dnsPrefix": "agentpublic1",
        "ports": [
          80,
          443,
          8080
        ],
        "secrets":[
          {
            "sourceVault":{
              "id":"vaultId"
            },
            "vaultCertificates":[
              {
                "certificateUrl" :"http://url2"
              }
            ]
          }
        ]
      }
    ],
    "linuxProfile": {
      "adminUsername": "azureuser",
      "ssh": {
        "publicKeys": [
          {
            "keyData": "ssh-rsa PUBLICKEY azureuser@linuxvm"
          }
        ]
      }
    }
  }
}